{
  "name": "chrome-bookmark-sync",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Sync bookmarks between Chrome and Firefox browsers",
  "homepage_url": "https://github.com/bmccarthy/chrome-bookmark-sync",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "bookmarks"
  ]
}